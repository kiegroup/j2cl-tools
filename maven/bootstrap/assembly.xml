<assembly xmlns="http://maven.apache.org/ASSEMBLY/2.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/ASSEMBLY/2.0.0 http://maven.apache.org/xsd/assembly-2.0.0.xsd">
  <id>jszip</id>
  <formats>
    <format>zip</format>
  </formats>
  <includeBaseDirectory>false</includeBaseDirectory>
  <fileSets>
    <fileSet>
      <directory>../../../closure-library/closure/goog/</directory>
      <includes>
        <!--
        List generated by running (combine two dashes before the "root" arg)
        $ closure/bin/build/closurebuilder.py - -root . -i closure/goog/base.js -i closure/goog/math/long.js
        from in closure-library.
        -->
        <include>base.js</include>
        <include>dom/nodetype.js</include>
        <include>debug/error.js</include>
        <include>asserts/asserts.js</include>
        <include>reflect/reflect.js</include>
        <include>math/long.js</include>
      </includes>
      <outputDirectory>closure/goog</outputDirectory>
    </fileSet>
  </fileSets>
</assembly>